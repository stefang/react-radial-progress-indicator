  "main": "dist/index.js",
  "author": "Tim Brayshaw <tim@brayshaw.com>",
  "license": "MIT",
  "homepage": "https://github.com/twisty/react-radial-progress-indicator#readme",
  "keywords": [
    "canvas",
    "circular-gauge",
